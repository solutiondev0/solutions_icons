{
    "name": "soluti0n-icons",
    "version": "1.0.0",
    "description": "Soluti0n icon library for React",
    "author": "soluti0n.dev",
    "license": "ISC",
    "files": ["dist"],
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.cjs.js",
            "types": "./dist/index.d.ts"
        },
        "./client": {
            "import": "./dist/client.esm.js",
            "require": "./dist/client.cjs.js",
            "types": "./dist/index.d.ts"
        }
    },
    "types": "dist/index.d.ts",
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && tsc --project tsconfig.json && rollup -c",
        "build:watch": "rollup -c -w",
        "prepublishOnly": "npm run build",
        "pack": "npm run build && npm pack"
    },
    "peerDependencies": {
        "react": "^18 || ^19",
        "react-dom": "^18 || ^19"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@types/rollup": "^0.51.4",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "rimraf": "^6.0.1",
        "rollup": "^4.50.1",
        "rollup-plugin-dts": "^6.2.3",
        "tslib": "^2.8.1",
        "typescript": "^5.9.2"
    },
    "keywords": [
        "react",
        "icons",
        "svg",
        "react-icons",
        "ui",
        "business",
        "education",
        "sysadmin",
        "media",
        "graphics",
        "programming",
        "opensource"
    ]
}
